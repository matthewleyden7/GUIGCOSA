<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="controllers.UpdateCustomerController">
   <children>
      <AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label layoutX="222.0" layoutY="43.0" text="Update Customer">
               <font>
                  <Font name="Arial" size="18.0" />
               </font>
            </Label>
            <GridPane layoutX="140.0" layoutY="95.0" prefHeight="193.0" prefWidth="332.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="Name" GridPane.rowIndex="1" />
                  <Label text="Address" GridPane.rowIndex="2" />
                  <Label text="Phone" GridPane.rowIndex="3" />
                  <TextField fx:id="nameField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <TextField fx:id="addressField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <TextField fx:id="phoneField" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <Label text="Country" GridPane.rowIndex="5" />
                  <ComboBox fx:id="countryBox" onAction="#filterStates" prefHeight="25.0" prefWidth="173.0" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                  <Label text="Zip" GridPane.rowIndex="4" />
                  <TextField fx:id="zipField" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                  <Label text="State/Province" GridPane.rowIndex="6" />
                  <ComboBox fx:id="stateBox" prefHeight="25.0" prefWidth="170.0" GridPane.columnIndex="1" GridPane.rowIndex="6" />
                  <Label text="Customer ID" />
                  <TextField fx:id="idField" GridPane.columnIndex="1" />
               </children>
            </GridPane>
            <HBox alignment="TOP_RIGHT" layoutX="358.0" layoutY="341.0" prefHeight="56.0" prefWidth="200.0">
               <children>
                  <Button fx:id="cancelBtn" mnemonicParsing="false" onAction="#cancelBtnPressed" text="Cancel" />
               </children>
            </HBox>
            <HBox alignment="TOP_RIGHT" layoutX="300.0" layoutY="302.0" prefHeight="17.0" prefWidth="174.0" spacing="25.0">
               <children>
                  <Button fx:id="updateBtn" mnemonicParsing="false" onAction="#updateBtnPressed" text="Update" />
               </children>
            </HBox>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
