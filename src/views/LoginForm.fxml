          <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" />
        </rowConstraints>
         <children>
            <TextField fx:id="userName" GridPane.columnIndex="1" />
            <Label fx:id="uNameLabel" prefHeight="16.0" prefWidth="108.0">
               <font>
                  <Font name="Arial" size="14.0" />
               </font>
            </Label>
            <PasswordField fx:id="password" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label fx:id="pWordLabel" prefHeight="17.0" prefWidth="109.0" GridPane.rowIndex="1">
               <font>
                  <Font name="Arial" size="14.0" />
               </font>
            </Label>
            <Button fx:id="loginBtn" mnemonicParsing="false" onAction="#loginBtnPressed" prefHeight="25.0" prefWidth="84.0" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="2" />
         </children>
      </GridPane>
      <Label fx:id="zoneLabel" alignment="CENTER_RIGHT" contentDisplay="RIGHT" layoutX="122.0" layoutY="307.0" prefHeight="17.0" prefWidth="309.0" />
      <Label fx:id="localTimeLabel" alignment="CENTER_RIGHT" contentDisplay="RIGHT" layoutX="122.0" layoutY="324.0" prefHeight="17.0" prefWidth="309.0" />
      <Label fx:id="businessTimeLabel" alignment="CENTER_RIGHT" contentDisplay="RIGHT" layoutX="122.0" layoutY="341.0" prefHeight="17.0" prefWidth="309.0" />
   </children>
</AnchorPane>
