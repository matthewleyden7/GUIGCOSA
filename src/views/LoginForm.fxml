<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.LoginFormController">
   <children>
      <Label fx:id="loginLabel" alignment="CENTER" layoutY="55.0" prefHeight="43.0" prefWidth="601.0">
         <font>
            <Font name="Constantia" size="36.0" />
         </font>
      </Label>
      <GridPane alignment="CENTER_RIGHT" layoutX="143.0" layoutY="129.0" prefHeight="156.0" prefWidth="288.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="126.0" minWidth="10.0" prefWidth="85.0" />
          <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" maxWidth="177.0" minWidth="10.0" prefWidth="177.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
